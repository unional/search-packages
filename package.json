{
  "name": "search-packages",
  "description": "Search packages on npm",
  "version": "0.0.0-development",
  "homepage": "https://github.com/unional/search-packages",
  "bugs": {
    "url": "https://github.com/unional/search-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/search-packages.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.es5.json && tsc -p tsconfig.esnext.json",
    "clean": "rimraf lib",
    "dc": "npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "verify": "npm run lint && npm run build && npm run dependency-check && jest --coverage",
    "watch": "jest --watch"
  },
  "devDependencies": {
    "@unional/devpkg-node": "^1.2.3",
    "assertron": "^6.1.0"
  }
}
