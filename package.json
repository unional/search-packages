{
  "name": "search-packages",
  "version": "0.0.0-development",
  "description": "Search packages on npm",
  "homepage": "https://github.com/unional/search-packages",
  "bugs": {
    "url": "https://github.com/unional/search-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/search-packages.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "libm/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.es5.json && tsc -p tsconfig.esnext.json",
    "clean": "rimraf lib libm",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "eslint --ext=js,ts .",
    "test": "jest",
    "verify": "yarn lint && yarn build && yarn dependency-check && jest --coverage",
    "watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@unional/devpkg-node": "^1.4.2",
    "assertron": "^7.1.2"
  }
}
